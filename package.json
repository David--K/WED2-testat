{
  "name": "testat1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -b && copyfiles ./src/views/*.hbs ./build/ && copyfiles ./src/public/stylesheets/style.sass ./build/",
    "start": "node ./src/bin/www",
    "start:build": "npm run build && node ./build/src/bin/www.js",
    "start:dev": "node -r dotenv/config -r tsconfig-paths/register -r ts-node/register ./src/bin/www.ts",
    "prettier": "prettier --config ./.prettierrc.js --write \"src/**/*.ts\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.13",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/http-errors": "^1.8.0",
    "@types/mocha": "^8.0.3",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.5",
    "@types/node-sass-middleware": "0.0.31",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
